-- Expected MD5 checksum of the loadstring command
local expectedChecksum = "ae482299e72b0cce80f9c59d25f93a93"

-- Function to calculate MD5 checksum
local function calculateMD5(str)
    return syn.crypt.hash("md5", str)
end

-- Fetch the source of the script
local scriptSource = game:HttpGet("https://raw.githubusercontent.com/NarcissistDev/Celestial/main/test2.lua")

-- Calculate the checksum of the current script
local currentChecksum = calculateMD5(scriptSource)

-- Verify the checksum
if currentChecksum == expectedChecksum then
    print("Checksum verified. Executing the script...")
    
    -- Place your actual script content here
    -- For demonstration, let's print a message
    print("The loadstring has been verified and is executing properly.")
else
    -- Kick the player if the checksum does not match
    game.Players.LocalPlayer:Kick("Modified loadstring detected.")
end
